{
  "$schema": "https://opencode.ai/config.json",
  "agent": {
    "browser": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "git": {
      "model": "github_copilot/gpt-5-mini",
    },
    "code-reader": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "os": {
      "model": "github_copilot/gpt-5-mini",
    },
    "websearch": {
      "model": "google/antigravity-gemini-3-flash",
    },
  },
  "instructions": ["AGENTS.md", "CLAUDE.md"],
  "keybinds": {
    "app_exit": "ctrl+d,<<leader>>q"
  },
  "mcp": {
    "chrome": {
      "type": "local",
      "command": ["npx", "-y", "chrome-devtools-mcp@latest"]
    },
    "context7": {
      "type": "local",
      "command": ["/home/me/.nvm/versions/node/v22.17.0/bin/context7-mcp"]
    },
    "excel": {
      "type": "local",
      "command": ["/home/me/excel-mcp-server/.venv/bin/excel-mcp-server", "stdio"]
    },
    "filesystem": {
      "type": "local",
      "command": [
        "/home/me/mcp-filesystem/.venv/bin/python",
        "/home/me/mcp-filesystem/run_server.py",
        "/home/me"
      ]
    },
    "git": {
      "type": "local",
      "command": [
        "/home/me/mcp-git/.venv/bin/mcp-server-git"
      ]
    },
    "websearch": {
      "type": "local",
      "command": [
        "env",
        "USE_PROXY=true",
        "PROXY_URL=http://localhost:1234",
        "HTTP_PROXY=http://localhost:1234",
        "HTTPS_PROXY=http://localhost:1234",
        "MODE=stdio",
        "npx",
        "-y",
        "open-websearch"
      ]
    }
  },
  "model": "github-copilot/claude-sonnet-4.5",
  "plugin": ["opencode-pty", "@plannotator/opencode", "@zenobius/opencode-skillful", "opencode-antigravity-auth@latest"],
  "provider": {
    "litellm": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "GitHub Copilot (GHE)",
      "options": {
        "baseURL": "http://127.0.0.1:4000/v1",
        "apiKey": "sk-1234"
      },
      "models": {
        "claude-haiku-4.5": {
          "id": "github_copilot/claude-haiku-4.5",
          "name": "Claude Haiku 4.5 (GHE)",
          "attachment": true,
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 200000,
            "output": 200000
          }
        },
        "claude-sonnet-4.5": {
          "id": "github_copilot/claude-sonnet-4.5",
          "name": "Claude Sonnet 4.5 (GHE)",
          "attachment": true,
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 200000,
            "output": 200000
          }
        },
        "claude-opus-4.5": {
          "id": "github_copilot/claude-opus-4.5",
          "name": "Claude Opus 4.5 (GHE)",
          "attachment": true,
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 200000,
            "output": 200000
          }
        },
        "gpt-5-mini": {
          "id": "github_copilot/gpt-5-mini",
          "name": "GPT 5 Mini (GHE)",
          "attachment": true,
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 128000,
            "output": 128000
          }
        },
        "gpt-5.2": {
          "id": "github_copilot/gpt-5.2",
          "name": "GPT 5.2 (GHE)",
          "attachment": true,
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 400000,
            "output": 400000
          }
        },
        "gpt-5.2-codex": {
          "id": "github_copilot/gpt-5.2-codex",
          "name": "GPT 5.2 Codex (GHE)",
          "attachment": true,
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 400000,
            "output": 400000
          }
        },
        "gemini-2.5-pro": {
          "id": "github_copilot/gemini-2.5-pro",
          "name": "Gemini 2.5 Pro (GHE)",
          "attachment": true,
          "reasoning": true,
          "temperature": true,
          "tool_call": true,
          "limit": {
            "context": 1000000,
            "output": 1000000
          }
        }
      }
    },
    "ollama": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Ollama (local)",
      "options": {
        "baseURL": "http://localhost:11434/v1"
      },
      "models": {
        "cogito": {
          "id": "cogito:8b",
          "name": "Cogito (Language)",
          "attachment": true,
          "limit": {
            "context": 16000,
            "output": 16000
          },
          "reasoning": true,
          "temperature": true,
          "tool_call": true
        },
        "qwen-2-5-coder": {
          "id": "qwen2.5-coder:latest",
          "name": "Qwen 2.5 (Coder)",
          "attachment": false,
          "limit": {
            "context": 16000,
            "output": 16000
          },
          "reasoning": false,
          "temperature": true,
          "tool_call": false
        },
        "qwen-3": {
          "id": "qwen3:14b",
          "name": "Qwen 3 (Tools)",
          "attachment": true,
          "limit": {
            "context": 12000,
            "output": 12000
          },
          "reasoning": true,
          "temperature": true,
          "tool_call": true
        }
      }
    },
    "google": {
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "low": { "thinkingLevel": "low" },
            "high": { "thinkingLevel": "high" }
          }
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "minimal": { "thinkingLevel": "minimal" },
            "low": { "thinkingLevel": "low" },
            "medium": { "thinkingLevel": "medium" },
            "high": { "thinkingLevel": "high" }
          }
        },
        "antigravity-claude-sonnet-4.5": {
          "name": "Claude Sonnet 4.5 (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "antigravity-claude-sonnet-4.5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } }
          }
        },
        "antigravity-claude-opus-4.5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } }
          }
        },
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "gemini-2.5-pro": {
          "name": "Gemini 2.5 Pro (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "gemini-3-flash-preview": {
          "name": "Gemini 3 Flash Preview (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "gemini-3-pro-preview": {
          "name": "Gemini 3 Pro Preview (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        }
      }
    }
  },
  "tools": {
    "chrome_*": false,
    "codesearch": false,
    "context7_*": false,
    "edit": false,
    "excel_*": false,
    "external_directory": false,
    "filesystem_*": false,
    "git_*": false,
    "glob": false,
    "google_search": false,
    "grep": false,
    "list": false,
    "lsp": false,
    "pty_*": false,
    "read": false,
    "skill": false,
    "websearch_*": false,

    // Build agent breaks if this tool is not enabled
    "bash": true,
    "webfetch": true
  }
}
